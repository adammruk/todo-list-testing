{
  "extends": [
    "react-app",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/jsx-tag-spacing": [
      1,
      {
        "beforeSelfClosing": "always"
      }
    ],
    "quotes": [
      1,
      "single"
    ],
    "jsx-quotes": [
      1,
      "prefer-double"
    ]
  }
}